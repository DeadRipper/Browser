<Window x:Class="Browser.History"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Browser"
        mc:Ignorable="d"
        Title="History"
        Height="{Binding SystemParameters.PrimaryScreenheight}" 
        Width="{Binding SystemParameters.PrimaryScreenwidth}">
    <Grid Margin="0,3,0,0" Height="498" VerticalAlignment="Top">
        <ListBox x:Name="listbox" HorizontalAlignment="Left" Height="478" Margin="10,10,0,0" VerticalAlignment="Top" Width="740" SelectionChanged="listbox_SelectionChanged"/>
    </Grid>
</Window>
